(function($, document, window, undefined) {
	
	$(document).ready(function() {
		if($('body').hasClass('front')) {
			$('#node-6849, #node-6850, #node-6846, #node-6847, #node-6848 .light_border_top').css({
				cursor: 'pointer'
			}).on('click', function(e) {
				e.preventDefault();
				var href = $(this).find('a').first().attr('href');
				if(href && href !== '#') {
					window.location.assign(href);
				}
			});

			$('#views_slideshow_cycle_teaser_section_home_page_slideshow-panel_pane_1 a').addClass("homepageSliderLink");
			$('div#node-6848 a').addClass("homepageToolsOnHP");
			$('div#node-6848 a strong').addClass("homepageToolsOnHP");
			$('div.homepage-event-box a').addClass("homepageEventBox");
			$('div.homepage-event-box a span').addClass("homepageEventBox");
			$('div.views-field-field-registration-link a').addClass("homepageEventBox");

		}




	});

})(jQuery, document, window, undefined);